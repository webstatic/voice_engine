{
  "name": "jisho",
  "version": "2.0.1",
  "description": "A japanese kanji dictionary",
  "keywords": [
    "japanese",
    "kanji"
  ],
  "main": "index.js",
  "bin": {
    "jisho": "index.js"
  },
  "scripts": {
    "start": "node index.js",
    "fix": "npm run format; npm run lint",
    "format": "prettier --write index.js lib/**.js",
    "lint": "eslint --fix index.js lib/**.js"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "https://git.sr.ht/~siegfriedehret/jisho"
  },
  "author": "Siegfried Ehret <siegfried@ehret.me>",
  "license": "MIT",
  "homepage": "https://git.sr.ht/~siegfriedehret/jisho",
  "dependencies": {
    "chalk": "^4.1.0",
    "meow": "^9.0.0",
    "sqlite3": "^5.0.1"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "prettier": "^2.2.1"
  }
}
